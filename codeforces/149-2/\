#include<cstdio>
#include<string>
#include<map>
using namespace std;

int main()
{
  pair<int, int> start;
  pair<int, int> final;

  scanf("%d %d %d %d", &start.first, &start.second, &start.first, &start.second);

  int n;

  map<unsigned long long, bool> allowed;

  while(n--)
  {
    int row, start, end;
    scanf("%d %d %d", &row, &start, &end);

    int row_prefix = row * 10e10;

    for(int i = start; i <= end; i++)
    {
      allowed[row_prefix + i]
    }
  }

  return 0;
}
